        <record id="nh_renal_context" model="nh.clinical.context">
            <field name="name">renal</field>
            <field name="models">['nh.clinical.location']</field>
        </record>
        <!--Main Partner-->
        <record model="res.company" id="base.main_company">
            <field name="name">Lister Hospital</field>
        </record>
        <record model="res.partner" id="base.main_partner">
            <field name="name">Lister Hospital</field>
            <field name="email">generalenquiries.enh-tr@nhs.net</field>
            <field name="website">http://www.enherts-tr.nhs.uk/</field>
            <field name="image">
                
            </field>
        </record>

        <!-- Location - Point of Service - Hospital -->
        <record id="nhc_lister_location_lh" model="nh.clinical.location">
