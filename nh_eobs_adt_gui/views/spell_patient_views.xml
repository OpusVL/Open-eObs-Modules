<openerp>
    <data>
        <record model="ir.ui.view" id="view_spell_patient_form">
                <field name="name">patient Form View</field>
                <field name="model">nh.clinical.patient</field>
                <field name="arch" type="xml">
                    <form string="patient Form View" delete="0">
                        <sheet>
                            <group>
                                <field name="given_name" required="True"/>
                                <field name="middle_names" />
                                <field name="family_name" required="True"/>
                                <field name="dob" widget="date"/>
                                <field name="sex" />
                                <field name="gender" />
                                <field name="ethnicity" />
                                <field name="patient_identifier" required="True"/>
                                <field name="other_identifier" />
                            </group>
                        </sheet>
                    </form>
                </field>
            </record>
    </data>
</openerp>