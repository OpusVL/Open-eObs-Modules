<openerp>
    <data>
        <!--UI changes-->
        <template id="assets_backend" name="lth assets" inherit_id="web.assets_backend">
            <xpath expr="." position="inside">
                <script type="text/javascript" src="/nh_eobs_bhft/static/src/js/nh_eobs_bhft.js"></script>
            </xpath>
        </template>
        <!--Main Partner-->
        <record model="res.company" id="base.main_company">
            <field name="name">Burton Hospitals Foundation Trust</field>
        </record>
        <record model="res.partner" id="base.main_partner">
            <field name="name">Burton Hospitals Foundation Trust</field>
            <field name="email">PALS@burtonft.nhs.uk</field>
            <field name="website">http://www.burtonhospitals.nhs.uk/</field>
            <field name="image"></field>
        </record>
        
    </data>
</openerp>